{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "manifest_version": 2,
  "page_action": {
      "default_popup": "hello.html",
      "default_icon": {
        "16": "hello.png",
    },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
}
